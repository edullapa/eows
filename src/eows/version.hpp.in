#ifndef __EOWS_EOWS_VERSION_HPP__
#define __EOWS_EOWS_VERSION_HPP__

//! Major version of EOWS (or version).
#define EOWS_VERSION_MAJOR @EOWS_VERSION_MAJOR@

//! Minor version of EOWS (or revision).
#define EOWS_VERSION_MINOR @EOWS_VERSION_MINOR@

//! Patched version of EOWS (or patch).
#define EOWS_VERSION_PATCH @EOWS_VERSION_PATCH@

//! Release status: alpha, beta, rc (release candidate), stable.
#define EOWS_VERSION_STATUS "@EOWS_VERSION_STATUS@"

//! A string description of EOWS version.
#define EOWS_VERSION_STRING "@EOWS_VERSION_STRING@"

//! EOWS version number as a 32-bit integer.
#define EOWS_VERSION_NUMBER @EOWS_VERSION_NUMBER@

#endif  // __EOWS_EOWS_VERSION_HPP__
